{
  "name": "renu",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install && pnpm db:generate",
    "db:generate": "turbo run db:generate",
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck",
    "format": "dprint fmt",
    "check:everything": "pnpm run typecheck lint test"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@effect/language-service": "0.0.21",
    "@types/node": "20.5.9",
    "dprint": "0.40.2",
    "eslint": "8.48.0",
    "eslint-config-custom": "workspace:*",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "next": "13.4.19",
    "turbo": "1.10.13",
    "typescript": "5.2.2"
  },
  "packageManager": "pnpm@8.6.12",
  "dependencies": {
    "@effect/data": "0.18.3",
    "@effect/io": "0.39.1",
    "@effect/match": "0.35.1",
    "@effect/schema": "0.34.0",
    "@sqlfx/pg": "0.15.1"
  },
  "engines": {
    "node": "18"
  },
  "pnpm": {
    "overrides": {
      "@fp-ts/data": "npm:@effect/data",
      "@effect/data": "$@effect/data",
      "@effect/io": "$@effect/io",
      "@effect/match": "$@effect/match",
      "@effect/schema": "$@effect/schema"
    }
  }
}
